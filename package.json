{
  "name": "semantic-release-vercel",
  "version": "1.0.3",
  "description": "A semantic-release plugin for deploying projects to Vercel",
  "sideEffects": false,
  "module": "./dist/index.mjs",
  "typings": "./dist/index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "esm": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "vite build",
    "test": "vitest --passWithNoTests",
    "test:headless": "vitest run --coverage --passWithNoTests",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "release": "semantic-release",
    "prepare": "husky"
  },
  "dependencies": {
    "@semantic-release/error": "^4.0.0",
    "@vercel/build-utils": "^10.3.1",
    "@vercel/client": "15.1.2",
    "@vercel/routing-utils": "^5.0.4",
    "bytes": "^3.1.2",
    "lodash-es": "^4.17.21",
    "signale": "^1.4.0",
    "tslib": "^2.8.1"
  },
  "peerDependencies": {
    "semantic-release": ">=24.1.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.7.1",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.21.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@types/bytes": "^3.1.5",
    "@types/lodash-es": "^4.17.12",
    "@types/semantic-release__error": "^3.0.3",
    "@types/signale": "^1.4.7",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@vitest/coverage-v8": "^3.0.7",
    "commitlint": "^19.7.1",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.7",
    "typescript": "~5.8.0",
    "vite": "^6.2.0",
    "vite-plugin-checker": "^0.9.0",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.0.7"
  },
  "keywords": [
    "semantic-release",
    "vercel",
    "deployment",
    "release",
    "continuous-integration",
    "continuous-deployment",
    "automation",
    "ci-cd"
  ],
  "readme": "README.md",
  "author": "Iacopo Ciao",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KernelPanic92/semantic-release-vercel.git"
  },
  "bugs": {
    "url": "https://github.com/KernelPanic92/semantic-release-vercel/issues"
  },
  "packageManager": "pnpm@10.5.2"
}
